local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "Envix's Script Hub",
    LoadingTitle = "Celestial Hub",
    LoadingSubtitle = "by Envix",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "Big Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = true,
    KeySettings = {
        Title = "Celestial Hub",
        Subtitle = "Key System",
        Note = "Join the discord to get the key https://discord.gg/9sY9j79BYD",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"milothedog"}
    }
})

-- Player Tab
local PlayerTab = Window:CreateTab("Player", 4483362458)

local Slider = PlayerTab:CreateSlider({
    Name = "WalkSpeed",
    Range = {1, 500},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 16,
    Flag = "Slider1",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
})

-- Other Hubs Tab
local OtherTab = Window:CreateTab("Other Hubs", 4483362458)

local Button = OtherTab:CreateButton({
    Name = "DebugHub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/yofriendfromschool1/debugnation/main/decompilers%20and%20debugging/Debuggers.txt"))()
    end,
})
